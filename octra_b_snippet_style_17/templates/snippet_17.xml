<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="octra_style_17" inherit_id="octra_business_snippets.octra_business_snippets"
		name="octra Snippet" priority="17">
		<xpath
			expr="//div[@id='octra_snippet_structure']/div[hasclass('o_panel_body')]"
			position="inside">
			<t t-snippet="octra_b_snippet_style_17.octra_b_snippet_style_17_template" t-thumbnail="/octra_b_snippet_style_17/static/src/img/snippet_17.jpg"/>
		</xpath>
	</template>
	<template id="octra_b_snippet_style_17_template" name="expertise">
		<div class="oe_structure" />
		<section class="common-div">
		<div class="block">
			<section class="about_us_image_div">
				<div class="block">
					<section class="about_content_sub">
						<h3 class="ch_h1_class">Delightful Experience</h3>
						<p class="sep_span_hr" />
						<p class="chs_para common-div octra_chs_para">Lorem ipsum dolor sit amet, consectetur adipiscing
							elit, sed do eiusmod tempor incididunt ut labore et dolore magna
							aliqua. Ut enim ad minim veniam, quis nostrud exercitation
							ullamco laboris nisi ut aliquip ex ea commodo consequat.
						</p>
						<a href="#" class="octra_btn">

							Buy Theme

						</a>
					</section>
				</div>
			</section>
			<section class="div_whole_about_us">
				<div class="about_us_div_class_main_snapshot_div block">
					<section class="left_side_snapshot_div">
						<ul class='skills'>
							<t t-foreach="res_company.expertise_ids" t-as="expertise_data">
								<li class='progressbar2 lato-regular font-black'
									t-att-data-width='expertise_data.expertise'>
									<t t-esc="expertise_data.name"></t>
								</li>
							</t>
						</ul>
					</section>
					<div id="jquery-script-menu">

					</div>
				</div>

				<div class="oe_structure" />
				<section>

				</section>
			</section>
			</div>
		</section>


	</template>
</odoo>
